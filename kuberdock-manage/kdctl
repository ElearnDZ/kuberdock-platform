#!/usr/bin/env python

from __future__ import print_function

if __name__ == '__main__':
    import requests

    import logging
    logging.basicConfig(format='%(message)s')

    requests.packages.urllib3.disable_warnings(
        requests.packages.urllib3.exceptions.InsecureRequestWarning
    )

    from kdctllib.cli.main import main
    from kdctllib.cli import ippool
    from kdctllib.cli import nodes
    from kdctllib.cli import podapi
    from kdctllib.cli import predefined_apps
    from kdctllib.cli import pricing
    from kdctllib.cli import restore
    from kdctllib.cli import system_settings
    from kdctllib.cli import users

    main()
